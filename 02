#include <iostream>

using namespace std;

int main()
{
int a, b, c;
cout<<a<<" " << b << " " << c << " " << endl;
 cin>>a>>b>>c;
if ((a+b<c)||(a+c<b)||(b+c<a))
    cout << "f" << endl;
       else if (((b*b)+(c*c)-(a*a)==0)||((b*b)+(a*a)-(c*c)==0)||((a*a)+(c*c)-(b*b)==0))
        cout << "right" << endl;
 else if ( ( ((b*b)+(c*c))-(a*a)>0)&&(((b*b)+(a*a))-(c*c)>0)&&(((a*a)+(c*c))-(b*b)>0))
       cout << "acuat" << endl;
 else
         cout << "obtuse" << endl;

return 0;

}
